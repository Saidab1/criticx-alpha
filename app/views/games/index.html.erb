<h1>All Games</h1>
<table>
  <thead>
    <tr>
    <th>Name</th>
    <th>Rating</th>
    </tr>
  </thead>

  <tbody>
    <%@games.each do |game|%>
      <tr>
        <td><%= game.name%></td>
        <td><%= game.rating%></td>
        <td><%= link_to "Show", game_path(game) %></td>
      </tr>
    <% end %>
  </tbody>
</table>


