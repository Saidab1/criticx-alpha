<h1>All Companies</h1>
<%= link_to "New Company", new_company_path%>
<table>
  <thead>
    <tr>
    <th>Name</th>
    <th>Country</th>
    </tr>
  </thead>

  <tbody>
    <%@companies.each do |company|%>
      <tr>
        <td><%= company.name%></td>
        <td><%= company.country%></td>
        <td><%= link_to "Show", company_path(company) %></td>
        <td><%= link_to "Edit", edit_company_path(company)%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "Go to Home page", root_path%>



