<h1>All Companies</h1>
<table>
  <thead>
    <tr>
    <th>Name</th>
    <th>Country</th>
    </tr>
  </thead>

  <tbody>
    <%@companies.each do |company|%>
      <tr>
        <td><%= company.name%></td>
        <td><%= company.country%></td>
        <td><%= link_to "Show", company_path(company) %></td>
      </tr>
    <% end %>
  </tbody>
</table>



